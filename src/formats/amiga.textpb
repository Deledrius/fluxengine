comment: 'Amiga 880kB 3.5" DSDD'

image_reader {
	filename: "amiga.adf"
	type: IMG
}

image_writer {
	filename: "amiga.adf"
	type: IMG
}

layout {
	tracks: 80
	sides: 2
	layoutdata {
		sector_size: 512
		physical {
			start_sector: 0
			count: 11
		}
	}
}

encoder {
	amiga {}
}

decoder {
	amiga {}
}

filesystem {
	type: AMIGAFFS
}

