input {
	file {
		filename: "atarist820.img"
		img {
			format {
				sectors: 10
				sector_size: 512
			}
			tracks: 82
			sides: 2
		}
	}
}

output {
	disk {
		drive {}
	}
}

encoder {
	ibm {
		track_length_ms: 200
		clock_rate_khz: 250
		emit_iam: false
		gap0: 80
		gap2: 22
		gap3: 34
		sector_skew: "0123456789"
		swap_sides: true
	}
}

cylinders {
	start: 0
	end: 81
}

heads {
	start: 0
	end: 1
}

