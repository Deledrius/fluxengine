input {
	file {
		filename: "ibm720.img"
		img {
			format {
				sectors: 9
				sector_size: 512
			}
		}
	}
}

output {
	disk {
		drive {}
	}
}

encoder {
	ibm {
		track_length_ms: 200
		clock_rate_khz: 250
		sector_skew: "012345678"
	}
}

cylinders {
	start: 0
	end: 79
}

heads {
	start: 0
	end: 1
}

